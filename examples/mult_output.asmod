; Test program for output formatting
; Outputs three different values: 42, 100, 255

start:
    POB val1        ; Load first value (42)
    WYJSCIE         ; Output it
    
    POB val2        ; Load second value (100) 
    WYJSCIE         ; Output it
    
    POB val3        ; Load third value (255)
    WYJSCIE         ; Output it
    
    STP             ; Stop program

val1: RST 42        ; First test value
val2: RST 100       ; Second test value  
val3: RST 255       ; Third test value
